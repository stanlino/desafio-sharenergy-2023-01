generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model users {
  id       String @id @default(auto()) @map("_id") @db.ObjectId
  password String
  username String @unique
}

model clients {
  id      String @id @default(auto()) @map("_id") @db.ObjectId
  name    String
  email   String @unique
  phone   String
  address String
  cpf     String @unique
}
